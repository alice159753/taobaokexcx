/*搜索开始*/

.page {
  width: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  position:fixed;
  top:0rpx;
  left:0px;
  z-index:999;
}
.page .search {
  width: 100%;
  background-color: #f8a6cc;
  height: 100rpx;
  position: relative;
}
.page .search .inp {
  width: 500rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #aaaaaa;
  background-color: #fff;
  border-bottom-left-radius:30rpx;
  border-top-left-radius:30rpx;
  position: absolute;
  top: 50%;
  margin-top: -35rpx;
  left: 36rpx;
  padding-left: 30rpx;
}
.page .search .close {
  width: 150rpx;
  height: 70rpx;
  background-color: #fff;
  position: absolute;
  top: 50%;
  margin-top: -35rpx;
  left: 486rpx;
  z-index: 9999;
  border-bottom-right-radius:30rpx;
  border-top-right-radius:30rpx;
}
.page .search .close .close_pic {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 50%;
  margin-top: -13rpx;
  right: 30rpx;
}
.page .btn {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.page .btn .search_icon {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 30rpx;
  right: 40rpx;
}
/*搜索结束*/

.swiper {
  height: 400rpx;
  width: 100%;
  margin-top:100rpx;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.scroll_view_h_category
{
  white-space: nowrap; 
  display: flex;
  padding-top:10rpx;
  padding-bottom:10rpx;
}

.scroll_view_h_category .category_box
{
   display: inline-block;
   width: 150rpx;
}
.scroll_view_h_category image
{
  width: 80rpx; 
  height: 80rpx;
}
.scroll_view_h_category image:first-child
{
